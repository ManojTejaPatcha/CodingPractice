import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int[] a={4,-1,2,1};
	    long max=Long.MIN_VALUE;
	    int sum=0;
	    for(int i=0;i<a.length;i++)
	    {
	        sum+=a[i];
	        if(max<sum)
	        max=sum;
	        
	        if(sum<0)
	        sum=0;
	    }
	    System.out.print(max);
	    
	}
}
