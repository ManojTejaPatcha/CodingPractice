import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    int dummy=n;
	    int rev=0;
	    while(n!=0)
	    {
	        int a=n%10;
	        rev=rev*10+a;
	        n=n/10;
	    }
	    if(rev==dummy)
		System.out.println("Palindrome");
		else
		System.out.println("Not Palindrome");
	}
}
